RewriteEngine On
# przekierowanie dla przykÅ‚adu www -> bez www
RewriteCond %{HTTP_HOST} !^b-mbteam.netlify.app$ [NC]
RewriteRule ^(.*)$ http://b-mbteam.netlify.app/$1 [L,R=301]
RewriteRule ^index.php$ https://b-mbteam.netlify.app [R=301,L]
